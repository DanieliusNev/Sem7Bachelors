{
  "templates": [
    {
      "templateId": "template_1",
      "title": "Tip End – C-yoke",
      "pages": [
        {
          "pageId": "page1",
          "title": "WTG specifications",
          "fields": [
            {
              "id": "Owner",
              "type": "text",
              "label": "Customer / Owner",
              "placeholder": "Enter name"
            },
            {
              "id": "ProjectName",
              "type": "text",
              "label": "Project name",
              "placeholder": "Enter project name"
            },
            {
              "id": "ProjectNr",
              "type": "text",
              "label": "Project no.",
              "placeholder": "Enter project number"
            },
            {
              "id": "TurbineNr",
              "type": "dropdown",
              "label": "WTG no.",
              "options": ["WTG 1", "WTG 2", "WTG 3"]
            },
            {
              "id": "ComponentNo",
              "type": "text",
              "label": "Component no.",
              "placeholder": "Enter component number"
            }
          ]
        },
        {
          "pageId": "page2",
          "title": "Calibrated Tools",
          "fields": []
        },
        {
          "pageId": "page3",
          "title": "General Inspection",
          "fields": [
            { "type": "text", "label": "Nameplate identification", "placeholder": "Enter TR" },
            { "type": "checkbox", "label": "Check Welding for cracks" },
            { "type": "checkbox", "label": "Check Lifting eyes for cracks" },
            { "type": "checkbox", "label": "Check Damage to surfaces" }
          ]
        },
        {
          "pageId": "page4",
          "title": "Axle & Tagline Point Inspection",
          "fields": [
            { "type": "checkbox", "label": "Check Axles and connections" },
            { "type": "checkbox", "label": "Check that the tagline points are securely fastened" },
            { "type": "checkbox", "label": "Check if spot welds are intact. If there are no spot welds, then tagline points must be snug tight" }
          ]
        },
        {
          "pageId": "page5",
          "title": "HPU Inspection",
          "fields": [
            { "type": "checkbox", "label": "Check HPU condition" },
            { "type": "checkbox", "label": "Check that 1 end is secured with a bracket" },
            { "type": "checkbox", "label": "Check that another end is secured with pins" }
          ]
        },
        {
          "pageId": "page6",
          "title": "Leakage Inspection",
          "fields": [
            { "type": "yesno", "label": "Where any leakages found inside system or cylinder of HPU?" },
            { "type": "checkbox", "label": "Check that 1 end is secured with a bracket" },
            { "type": "text", "label": "Remarks for documentation", "placeholder": "Remarks" }
          ]
        }
      ]
    },
    {
